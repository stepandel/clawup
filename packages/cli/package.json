{
  "name": "clawup",
  "version": "2.5.0",
  "description": "Deploy and manage a fleet of OpenClaw AI agents on AWS or Hetzner",
  "bin": {
    "clawup": "dist/bin.js"
  },
  "files": [
    "dist",
    "infra",
    "scripts"
  ],
  "scripts": {
    "build": "tsc && node scripts/sync-infra.mjs",
    "build:bundle": "node scripts/bundle.mjs && node scripts/sync-infra.mjs",
    "prepublishOnly": "node scripts/bundle.mjs && node scripts/sync-infra.mjs",
    "watch": "tsc --watch",
    "postinstall": "node scripts/postinstall.mjs"
  },
  "dependencies": {
    "@clack/prompts": "^0.9",
    "@scarf/scarf": "^1.4.0",
    "commander": "^13.0",
    "picocolors": "^1.1",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@clawup/core": "workspace:*",
    "@types/node": "^20.0.0",
    "esbuild": "^0.27.3",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stepandel/clawup.git",
    "directory": "packages/cli"
  },
  "homepage": "https://clawup.sh",
  "exports": {
    "./lib/constants": "./dist/lib/constants.js",
    "./types": "./dist/types.js"
  }
}
